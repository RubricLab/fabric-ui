{
	"name": "fabric/ui",
	"version": "0.1.0",
	"private": true,
	"description": "Shared UI components and types for Views",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./src/index.ts",
			"require": "./src/index.ts",
			"types": "./src/index.ts"
		},
		"./types": {
			"import": "./src/types/index.ts",
			"require": "./src/types/index.ts",
			"types": "./src/types/index.ts"
		}
	},
	"files": ["src"],
	"scripts": {
		"dev": "echo 'UI package - no dev command needed'",
		"build": "echo 'UI package - no build needed for TypeScript source'",
		"lint": "bunx @biomejs/biome check .",
		"format": "bunx @biomejs/biome check --write ."
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"next": "^15.0.0"
	},
	"dependencies": {
		"@radix-ui/react-alert-dialog": "^1.1.14",
		"@radix-ui/react-checkbox": "^1.3.1",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.4",
		"@radix-ui/react-select": "^2.2.2",
		"@radix-ui/react-toggle": "^1.1.6",
		"@radix-ui/react-tooltip": "^1.2.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.503.0",
		"recharts": "^2.15.3",
		"sonner": "^2.0.3",
		"tailwind-merge": "^3.2.0",
		"zod": "^3.25.51"
	},
	"devDependencies": {
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"typescript": "^5"
	}
}
